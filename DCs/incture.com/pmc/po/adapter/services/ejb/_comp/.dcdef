<?xml version="1.0" encoding="UTF-8"?>
<development-component
		version="1.0.5"
		xmlns="http://xml.sap.com/2002/11/DevelopmentComponent"
		xmlns:IDX="urn:sap.com:DevelopmentComponent:1.0"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://xml.sap.com/2002/11/DevelopmentComponent dcdef.xsd">
	<name>pmc/po/adapter/services/ejb</name>
	<vendor>incture.com</vendor>
	<component-type>
		<vendor>sap.com</vendor>
		<type>J2EE</type>
		<sub-type>EJBModule</sub-type>
	</component-type>
	<properties>
		<facet-attributes>
			<facet facet-id="java" version="1.6"/>
		</facet-attributes>
		<folder-attributes xmlns="http://xml.sap.com/2002/11/DevelopmentComponent">
			<folder-attribute attribute="meta-inf" path="ejbModule/META-INF"/>
		</folder-attributes>
		<j2ee-attributes xmlns="http://xml.sap.com/2002/11/DevelopmentComponent">
			<jee-version value="5.0"/>
		</j2ee-attributes>
		<localization xmlns="http://xml.sap.com/2003/06/Localization" xmlns:ns0="http://xml.sap.com/2003/06/Localization">
			<domain>BC</domain>
			<originalLocale>en</originalLocale>
		</localization>
	</properties>
	<build-plugin>
		<dc-ref>
			<name>tc/bi/bp/ejbmodule</name>
			<vendor>sap.com</vendor>
		</dc-ref>
		<pp-ref>ejb</pp-ref>
	</build-plugin>
	<dependencies>
		<dependency>
			<dc-ref>
				<name>pmc/po/adapter/entities/ejb</name>
				<vendor>incture.com</vendor>
			</dc-ref>
			<pp-ref>client</pp-ref>
			<at-build-time/>
		</dependency>
		<dependency>
			<dc-ref>
				<name>pmc/po/adapter/entities/ejb</name>
				<vendor>incture.com</vendor>
			</dc-ref>
			<pp-ref>ejbjar</pp-ref>
			<at-build-time/>
		</dependency>
		<dependency>
			<dc-ref>
				<name>engine.jee5.facade</name>
				<vendor>sap.com</vendor>
			</dc-ref>
			<pp-ref>api</pp-ref>
			<at-build-time/>
		</dependency>
		<dependency>
			<dc-ref>
				<name>tc/bpem/facade/ear</name>
				<vendor>sap.com</vendor>
			</dc-ref>
			<pp-ref>api</pp-ref>
			<at-build-time/>
		</dependency>
		<dependency>
			<dc-ref>
				<name>tc/esi/esp/wsrm/facade</name>
				<vendor>sap.com</vendor>
			</dc-ref>
			<pp-ref>api</pp-ref>
			<at-build-time/>
		</dependency>
		<dependency>
			<dc-ref>
				<name>tc/je/sdo21/api</name>
				<vendor>sap.com</vendor>
			</dc-ref>
			<pp-ref>api</pp-ref>
			<at-build-time/>
		</dependency>
		<dependency>
			<dc-ref>
				<name>tc/je/usermanagement/api</name>
				<vendor>sap.com</vendor>
			</dc-ref>
			<pp-ref>api</pp-ref>
			<at-build-time/>
		</dependency>
		<dependency>
			<dc-ref>
				<name>tc/je/webservices/api</name>
				<vendor>sap.com</vendor>
			</dc-ref>
			<pp-ref>api</pp-ref>
			<at-build-time/>
		</dependency>
	</dependencies>
	<folders>
		<package-folder>.apt_generated</package-folder>
		<package-folder>ejbModule</package-folder>
	</folders>
	<access-control-list>
		<grant forwarding-allowed="true">
			<dc-ref>
				<name>*</name>
				<vendor>*</vendor>
			</dc-ref>
		</grant>
	</access-control-list>
</development-component>
